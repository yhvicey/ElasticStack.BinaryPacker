<Project>
    <Import Project="$([MSBuild]::GetPathOfFileAbove('$(MSBuildThisFile)', '$(MSBuildThisFileDirectory)../'))" />

    <PropertyGroup>
        <BinaryPackageDownloadUrlTemplate>https://artifacts.elastic.co/downloads/logstash/logstash-{BinaryPackageVersion}-{BinaryPackageOS}-{BinaryPackageArchitecture}{BinaryPackageExtension}</BinaryPackageDownloadUrlTemplate>
        <BinaryPackageExtension>.zip</BinaryPackageExtension>
        <BinaryPackageNameTemplate>logstash-{BinaryPackageOS}-{BinaryPackageArchitecture}-binary</BinaryPackageNameTemplate>
        <IsNestedPackage>True</IsNestedPackage>
    </PropertyGroup>

</Project>