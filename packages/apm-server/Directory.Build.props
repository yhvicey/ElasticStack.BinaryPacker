<Project>
    <Import Project="$([MSBuild]::GetPathOfFileAbove('$(MSBuildThisFile)', '$(MSBuildThisFileDirectory)../'))" />

    <PropertyGroup>
        <BinaryPackageDownloadUrlTemplate>https://artifacts.elastic.co/downloads/apm-server/apm-server-{BinaryPackageVersion}-{BinaryPackageOS}-{BinaryPackageArchitecture}{BinaryPackageExtension}</BinaryPackageDownloadUrlTemplate>
        <BinaryPackageNameTemplate>apm-server-{BinaryPackageOS}-{BinaryPackageArchitecture}-binary</BinaryPackageNameTemplate>
        <IsNestedPackage>True</IsNestedPackage>
    </PropertyGroup>

</Project>